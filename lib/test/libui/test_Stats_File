#!/usr/bin/env libui
#####
#
# test_Stats_File
#
#####

test_Stats_File () {
  local lfile; GetTmp -f lfile
  (LIBUI_STATS=true LIBUI_STATSFILE="${lfile}" libui -x w)
  LibuiPerformTest "grep '# libui stats on ' '${lfile}'"
  LibuiValidateTest -r ${?} 0 '# libui stats on .*'
  return ${?}
}

return 0

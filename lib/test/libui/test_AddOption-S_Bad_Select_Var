#!/usr/bin/env libui
#####
#
# test_AddOption-S_Bad_Select_Var
#
#####

test_AddOption-S_Bad_Select_Var () {
  LibuiPerformTest 'TERMINAL=true libui -x oS -Z a'
  LibuiValidateTest -r ${?} 2 '.*ERROR: The value provided for -Z \(Test S\) is not an available option value\. \(a\).*'
  return ${?}
}

return 0
